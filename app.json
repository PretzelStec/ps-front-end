{
  "expo": {
    "name": "Veiled",
    "slug": "login",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/VeiledLogo.png",
    "splash": {
      "image": "./assets/VeiledSplash (2).png",
      "resizeMode": "cover",
      "backgroundColor": "#404040"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.super.test"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/VeiledLogo.png",
        "backgroundColor": "#D15D5D"
      },
      "package": "com.super.test"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
